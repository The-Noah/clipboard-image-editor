{
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Clipboard Image Editor",
  "version": "0.1.0",
  "identifier": "com.noahdunbar.clipboard-image-editor",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Clipboard Image Editor",
        "width": 800,
        "height": 600,
        "resizable": false,
        "minimizable": false,
        "maximizable": false,
        "visible": false,
        "center": true,
        "zoomHotkeysEnabled": false,
        "transparent": true,
        "windowEffects": {
          "effects": ["acrylic"]
        }
      }
    ],
    "security": {
      "csp": null
    }
  }
}
